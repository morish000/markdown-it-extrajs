{
  "name": "marp-sample",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "html": "marp ./sample.md --engine ./marp.engine.mjs --html -o ./dist/sample.html",
    "html-nohtml": "marp ./sample-nohtml.md --engine ./marp.engine.mjs --html -o ./dist/sample-nohtml.html",
    "pdf": "marp ./sample.md --engine ./marp.engine.mjs --html --browser-timeout=12000 --pdf -o ./dist/sample.pdf",
    "pptx": "marp ./sample.md --engine ./marp.engine.mjs --html --browser-timeout=12000 --pptx -o ./dist/sample.pptx",
    "puppeteer": "node ./puppeteer.mjs",
    "playwright": "node ./playwright.mjs",
    "marp-core": "node ./marp-core.mjs",
    "marp-core-nohtml": "node ./marp-core-nohtml.mjs"
  },
  "type": "module",
  "keywords": [],
  "author": "morish000",
  "license": "MIT",
  "description": "Marp Sample",
  "devDependencies": {
    "@marp-team/marp-cli": "^4.2.3",
    "@marp-team/marp-core": "^4.2.0",
    "markdown-it-attrs": "^4.3.1",
    "playwright": "^1.57.0",
    "puppeteer": "^24.35.0",
    "@morish000/markdown-it-extrajs": "^0.0.5"
  }
}